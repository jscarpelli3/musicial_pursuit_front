eQuestion(3);\n        }\n      }\n      if (this.question !== \"\") {\n        this.show_question = true;\n        this.showQuestion(questionType);\n      }\n    },\n    showQuestion(type) {\n      console.log(type);\n      console.log(this.question);\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAE;IACLC,eAAe,EAAEC,MAAM;IACvBC,QAAQ,EAAEC;EACZ,CAAC;EACDC,IAAI,EAAE,OAAO;IACXC,SAAS,EAAE,EAAE;IACbC,SAAS,EAAE,GAAG;IACdC,WAAW,EAAE,GAAG;IAChBC,QAAQ,EAAE,EAAE;IACZC,UAAU,EAAE,EAAE;IACdC,aAAa,EAAE,KAAK;IACpBC,aAAa,EAAE,CAAC,oBAAoB,EAAE,iBAAiB,EAAE,kBAAkB,EAAE,MAAM,EAAE,mBAAmB,EAAE,mBAAmB,EAAE,sBAAsB,EAAE,iBAAiB,EAAE,oBAAoB,EAAE,kBAAkB,CAAC;IACnNC,KAAK,EAAE,EAAE;IACTC,YAAY,EAAE;EAChB,CAAC,CAAC;EACFC,OAAO,GAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACPC,UAAU,CAACC,MAAM,EAAEC,MAAM,EAAE;MACzB,IAAI,CAACC,KAAK,CAAC,SAAS,EAAEF,MAAM,EAAEC,MAAM;IACtC,CAAC;IACDE,YAAY,GAAG;MACb,IAAI,CAACD,KAAK,CAAC,WAAW;IACxB,CAAC;IACDE,WAAW,GAAG;MACZC,OAAO,CAACC,GAAG,CAAC,cAAc;IAC5B,CAAC;IACDT,cAAc,CAACU,SAAS,EAAE;MACxB;MACA;MACA,IAAIC,MAAK,GAAI,IAAI,CAAC1B,eAAe,CAAC2B,KAAK,CAAC,CAAC,CAAC,CAACC,SAAQ;MACnD,IAAIC,QAAO,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAC,GAAI,IAAI,CAAChC,eAAe,CAAC2B,KAAK,CAACM,MAAM;MAC3E,IAAIN,KAAI,GAAI,IAAI,CAAC3B,eAAe,CAAC2B,KAAK,CAACE,QAAQ,CAAC,CAACK,QAAO;MACxD,IAAIC,OAAM,GAAI,IAAI,CAACnC,eAAe,CAAC2B,KAAK,CAACE,QAAQ,CAAC,CAACO,eAAc;MACjE,IAAIC,IAAG,GAAIF,OAAO,CAACG,QAAQ,EAAC;MAC5B,IAAIC,YAAW,GAAIT,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAC,GAAIP,SAAS;MACvD;MACA,IAAIc,YAAW,KAAM,CAAC,EAAE;QACtB,IAAI,CAACd,SAAQ,GAAK,8BAA6BE,KAAM,aAAW;MAClE,OAAO,IAAIY,YAAW,KAAM,CAAC,EAAE;QAC7B,IAAI,CAAC/B,QAAO,GAAK,kBAAiBkB,MAAM,wBAAwBW,IAAK,GAAC;MACxE,OAAO,IAAIE,YAAW,KAAM,CAAC,EAAE;QAC7B,IAAI,CAAC/B,QAAO,GAAK,kBAAiBmB,KAAM,6CAA2C;MACrF,OAAO,IAAIY,YAAW,KAAM,CAAC,EAAE;QAC7B,IAAI,IAAI,CAACvC,eAAe,CAAC2B,KAAK,CAACE,QAAQ,CAAC,CAACW,QAAO,KAAM,IAAI,EAAE;UAC1D,IAAI,CAAChC,QAAO,GAAK,+BAA8BmB,KAAM,gBAAc;QACrE,OAAO;UACL,IAAI,CAACZ,cAAc
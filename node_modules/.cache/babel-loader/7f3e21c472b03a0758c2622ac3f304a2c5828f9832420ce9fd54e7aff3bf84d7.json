{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Client from '../services/api';\n// import { LoginUser } from '@/services/Auth';\n// const API_KEY = process.env.VUE_APP_AUDIODB_KEY\n\nexport default {\n  name: 'DashboardView',\n  props: {\n    user_handle: String,\n    user_id: Number\n  },\n  data: () => ({\n    user_profile: {},\n    watchlist: [],\n    has_watchlist: false\n  }),\n  components: {},\n  mounted() {\n    this.getUserInfo();\n  },\n  methods: {\n    async getUserInfo() {\n      const intId = this.user_id;\n      const Id = intId.toString();\n      const res = await axios.get(`http://localhost:3001/api/user/userprof/${Id}`);\n      // console.log(res.data)\n      this.user_profile = res.data;\n      this.watchlist = res.data.being_watched;\n      console.log(this.user_profile);\n      console.log(this.watchlist);\n      if (this.watchlist.length > 0) {\n        this.has_watchlist = true;\n      } else {\n        this.has_watchlist = false;\n      }\n    },\n    async deleteUser() {\n      const intId = this.user_id;\n      // const Id = intId.toString()\n      await Client.delete(`/user/${intId}`);\n      this.$router.push(`/bye`);\n    },\n    async removeFromList(being_watched, watcher) {\n      await Client.delete(`/user/${being_watched}/${watcher}`);\n      this.getUserInfo();\n    }\n  }\n};","map":{"version":3,"mappings":";AAmDA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,MAAK,MAAO,iBAAgB;AACnC;AACA;;AAEA,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,WAAW,EAAEC,MAAM;IACnBC,OAAO,EAAEC;EACX,CAAC;EACDC,IAAI,EAAE,OAAO;IACXC,YAAY,EAAE,CAAC,CAAC;IAChBC,SAAS,EAAE,EAAE;IACbC,aAAa,EAAE;EACjB,CAAC,CAAC;EACFC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,GAAG;IACR,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAW,GAAG;MAClB,MAAME,KAAI,GAAI,IAAI,CAACV,OAAM;MACzB,MAAMW,EAAC,GAAID,KAAK,CAACE,QAAQ,EAAC;MAC1B,MAAMC,GAAE,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAE,2CAA0CH,EAAG,EAAC;MAC3E;MACA,IAAI,CAACR,YAAW,GAAIU,GAAG,CAACX,IAAG;MAC3B,IAAI,CAACE,SAAQ,GAAIS,GAAG,CAACX,IAAI,CAACa,aAAY;MACtCC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,YAAY;MAC7Ba,OAAO,CAACC,GAAG,CAAC,IAAI,CAACb,SAAS;MAC1B,IAAI,IAAI,CAACA,SAAS,CAACc,MAAK,GAAI,CAAC,EAAE;QAC7B,IAAI,CAACb,aAAY,GAAI,IAAG;MAC1B,OAAO;QACL,IAAI,CAACA,aAAY,GAAI,KAAI;MAC3B;IACF,CAAC;IACD,MAAMc,UAAU,GAAG;MACjB,MAAMT,KAAI,GAAI,IAAI,CAACV,OAAM;MACzB;MACA,MAAML,MAAM,CAACyB,MAAM,CAAE,SAAQV,KAAM,EAAC;MACpC,IAAI,CAACW,OAAO,CAACC,IAAI,CAAE,MAAK;IAC1B,CAAC;IACD,MAAMC,cAAc,CAACR,aAAa,EAAES,OAAO,EAAE;MAC3C,MAAM7B,MAAM,CAACyB,MAAM,CAAE,SAAQL,aAAc,IAAGS,OAAQ,EAAC;MACvD,IAAI,CAAChB,WAAW,EAAC;IACnB;EACF;AACF","names":["axios","Client","name","props","user_handle","String","user_id","Number","data","user_profile","watchlist","has_watchlist","components","mounted","getUserInfo","methods","intId","Id","toString","res","get","being_watched","console","log","length","deleteUser","delete","$router","push","removeFromList","watcher"],"sourceRoot":"","sources":["/Users/jscarpelli/ga_seir919/projects/musical_pursuit/musicial_pursuit_front/musical_pursuit/src/components/Dashboard.vue"],"sourcesContent":["<template>\n  <div class=\"profile-space\">\n    <div class=\"user-info\">\n      <h1>{{ user_profile.handle }}</h1>\n      <h3>{{ user_profile.city }}</h3>\n    </div>\n    <div class=\"user-stats\">\n      <h2>Your Level: {{ user_profile.current_level }}</h2>\n    </div>\n    <div class=\"play-btn-div\">\n      <router-link to=\"game\"><button class=\"play-btn\">PLAY NOW!</button></router-link>\n    </div>\n    <div class=\"find-user\">\n      <div class=\"find-user\">\n        <router-link to=\"/viewusers\"><button>View All Users</button></router-link>\n        <router-link to=\"/viewbarks\"><button>View Your Barks</button></router-link>\n      </div>\n    </div>\n    <div class=\"user-watch\">\n      <h3 class=\"watch-header\">Your watchlist:</h3>\n      <div v-if=\"has_watchlist\">\n        <div class=\"temp\">\n          <div class=\"watch-display\" :key=\"watch.id\" v-for=\"watch in watchlist\">\n            <div class=\"comp-div\">\n              <h4 class=\"competitor-name\"><b>Name: </b>{{ watch.handle }} </h4>\n              <h5 class=\"competitor-score\"><b></b>Their Score: {{ watch.total_score }} </h5>\n              <button @click=\"removeFromList(watch.id, user_id)\">REMOVE </button>\n              <button @click=\"makeBark(watch.id, user_id)\">BARK!</button>\n            </div>\n\n          </div>\n        </div>\n      </div>\n      <div v-else class=\"user-watch\">\n        <h3 class=\"watch-header\">look like you're not watching anyone yet</h3>\n        <h4>Find some other players and watch their progress!</h4>\n        <div class=\"find-user\">\n          <router-link to=\"/viewusers\"><button>View All Users</button></router-link>\n        </div>\n        <!-- <button class=\"user-list-button\">Find Users</button> -->\n      </div>\n    </div>\n    <div class=\"delete-yourself\">\n      <p>If you've had enough of RockDog trivia... go ahead and delete yourself. We'll be sad you\n        left. But we still appreciate you playing!</p>\n      <button class=\"delete-profile-btn\" @click=\"deleteUser()\">Delete Yourself</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport Client from '../services/api'\n// import { LoginUser } from '@/services/Auth';\n// const API_KEY = process.env.VUE_APP_AUDIODB_KEY\n\nexport default {\n  name: 'DashboardView',\n  props: {\n    user_handle: String,\n    user_id: Number\n  },\n  data: () => ({\n    user_profile: {},\n    watchlist: [],\n    has_watchlist: false\n  }),\n  components: {},\n  mounted() {\n    this.getUserInfo()\n  },\n  methods: {\n    async getUserInfo() {\n      const intId = this.user_id\n      const Id = intId.toString()\n      const res = await axios.get(`http://localhost:3001/api/user/userprof/${Id}`)\n      // console.log(res.data)\n      this.user_profile = res.data\n      this.watchlist = res.data.being_watched\n      console.log(this.user_profile)\n      console.log(this.watchlist)\n      if (this.watchlist.length > 0) {\n        this.has_watchlist = true\n      } else {\n        this.has_watchlist = false\n      }\n    },\n    async deleteUser() {\n      const intId = this.user_id\n      // const Id = intId.toString()\n      await Client.delete(`/user/${intId}`)\n      this.$router.push(`/bye`)\n    },\n    async removeFromList(being_watched, watcher) {\n      await Client.delete(`/user/${being_watched}/${watcher}`)\n      this.getUserInfo()\n    }\n  }\n}\n</script>\n\n<style scoped>\n.profile-space {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.user-info {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n}\n\n.watch-header {\n  margin: 2vh 0 3vh 0;\n  text-shadow: 2px 1px 3px black;\n}\n\n.delete-yourself {\n  border-top: 1px solid black;\n  border-bottom: 1px solid black;\n  margin: 2vh 4vw;\n  padding-bottom: 2vh;\n}\n\n.watch-display {\n  display: grid;\n  grid-template-columns: 1fr/1fr;\n  margin: 2vh 4vw 2vh 4vw;\n  /* border: 1px solid black; */\n  /* margin: 2vw; */\n  box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.347);\n  border-radius: 10px;\n  background-color: rgba(240, 255, 255, 0.195);\n  /* border-radius: 8px; */\n}\n\n.user-stats {\n  margin: 2vh;\n}\n\n.play-btn-div {\n  margin: 2vh;\n  /* padding: 2vh */\n}\n\n.play-btn {\n  width: 80vw;\n  height: 8vh;\n  background-color: rgb(244, 118, 28);\n  text-shadow: 2px 1px 3px;\n}\n\n/* .user-barks {\n  margin: 2vh;\n}\n\n.barks-display {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: flex-start;\n  align-content: flex-start;\n} */\n\nbutton {\n  width: 40vw;\n  height: 4vh;\n  background-color: rgba(227, 247, 247, 0.467);\n  border-radius: 8px;\n  /* margin-bottom: 2vh; */\n  margin: 2vh 4vw;\n}\n\n/* .bark-header {\n  margin-bottom: 2vh;\n} */\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  margin: 0;\n  padding: 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
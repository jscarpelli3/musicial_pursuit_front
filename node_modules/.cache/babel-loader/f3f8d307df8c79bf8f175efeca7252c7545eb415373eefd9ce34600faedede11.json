{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Client from '../services/api';\nexport default {\n  name: 'DashboardView',\n  props: {\n    user_handle: String,\n    user_id: Number,\n    auth: Boolean\n  },\n  data: () => ({\n    user_profile: {},\n    watchlist: [],\n    has_watchlist: false,\n    new_bark: false\n  }),\n  components: {},\n  mounted() {\n    this.getUserInfo();\n  },\n  methods: {\n    async getUserInfo() {\n      console.log(this.auth);\n      const intId = this.user_id;\n      const Id = intId.toString();\n      const res = await Client.get(`/user/userprof/${Id}`);\n      this.user_profile = res.data;\n      this.$emit('User', this.user_profile);\n      this.watchlist = res.data.being_watched;\n      if (this.watchlist.length > 0) {\n        this.has_watchlist = true;\n      } else {\n        this.has_watchlist = false;\n      }\n    },\n    async deleteUser() {\n      const intId = this.user_id;\n      await Client.delete(`/user/${intId}`);\n      this.$router.push(`/bye`);\n    },\n    async removeFromList(being_watched, watcher) {\n      await Client.delete(`/user/${being_watched}/${watcher}`);\n      this.getUserInfo();\n    },\n    makeBark(id, handle) {\n      this.$router.push(`/newbark/${id}/${handle}`);\n    }\n  }\n};","map":{"version":3,"mappings":";AAgEA,OAAOA,MAAK,MAAO,iBAAgB;AAEnC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,WAAW,EAAEC,MAAM;IACnBC,OAAO,EAAEC,MAAM;IACfC,IAAI,EAAEC;EACR,CAAC;EACDC,IAAI,EAAE,OAAO;IACXC,YAAY,EAAE,CAAC,CAAC;IAChBC,SAAS,EAAE,EAAE;IACbC,aAAa,EAAE,KAAK;IACpBC,QAAQ,EAAE;EACZ,CAAC,CAAC;EACFC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,GAAG;IACR,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAW,GAAG;MAClBE,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,IAAI;MACrB,MAAMa,KAAI,GAAI,IAAI,CAACf,OAAM;MACzB,MAAMgB,EAAC,GAAID,KAAK,CAACE,QAAQ,EAAC;MAC1B,MAAMC,GAAE,GAAI,MAAMvB,MAAM,CAACwB,GAAG,CAAE,kBAAiBH,EAAG,EAAC;MACnD,IAAI,CAACX,YAAW,GAAIa,GAAG,CAACd,IAAG;MAC3B,IAAI,CAACgB,KAAK,CAAC,MAAM,EAAE,IAAI,CAACf,YAAY;MACpC,IAAI,CAACC,SAAQ,GAAIY,GAAG,CAACd,IAAI,CAACiB,aAAY;MACtC,IAAI,IAAI,CAACf,SAAS,CAACgB,MAAK,GAAI,CAAC,EAAE;QAC7B,IAAI,CAACf,aAAY,GAAI,IAAG;MAC1B,OAAO;QACL,IAAI,CAACA,aAAY,GAAI,KAAI;MAC3B;IACF,CAAC;IACD,MAAMgB,UAAU,GAAG;MACjB,MAAMR,KAAI,GAAI,IAAI,CAACf,OAAM;MACzB,MAAML,MAAM,CAAC6B,MAAM,CAAE,SAAQT,KAAM,EAAC;MACpC,IAAI,CAACU,OAAO,CAACC,IAAI,CAAE,MAAK;IAC1B,CAAC;IACD,MAAMC,cAAc,CAACN,aAAa,EAAEO,OAAO,EAAE;MAC3C,MAAMjC,MAAM,CAAC6B,MAAM,CAAE,SAAQH,aAAc,IAAGO,OAAQ,EAAC;MACvD,IAAI,CAACjB,WAAW,EAAC;IACnB,CAAC;IACDkB,QAAQ,CAACC,EAAE,EAAEC,MAAM,EAAE;MACnB,IAAI,CAACN,OAAO,CAACC,IAAI,CAAE,YAAWI,EAAG,IAAGC,MAAO,EAAC;IAE9C;EACF;AACF","names":["Client","name","props","user_handle","String","user_id","Number","auth","Boolean","data","user_profile","watchlist","has_watchlist","new_bark","components","mounted","getUserInfo","methods","console","log","intId","Id","toString","res","get","$emit","being_watched","length","deleteUser","delete","$router","push","removeFromList","watcher","makeBark","id","handle"],"sourceRoot":"","sources":["/Users/jscarpelli/ga_seir919/projects/musical_pursuit/musicial_pursuit_front/musical_pursuit/src/components/Dashboard.vue"],"sourcesContent":["<template>\n  <div>\n    <div v-if=\"auth\" class=\"dashboard\">\n      <div class=\"profile-space\">\n        <div class=\"user-info\">\n          <h1>{{ user_profile.handle }}</h1>\n          <h4>from:{{ user_profile.city }}</h4>\n        </div>\n        <div class=\"user-stats\">\n          <h4>Current Level: {{ parseInt(user_profile.current_level) }}</h4>\n          <h4>All Time Level: {{ parseInt(user_profile.alltime_level) }}</h4>\n          <h4>High Session Score: {{ parseInt(user_profile.high_ses_score) }}</h4>\n          <h3>Total Score: {{ parseInt(user_profile.total_score) }}</h3>\n        </div>\n        <div class=\"play-btn-div\">\n          <router-link to=\"game\"><button class=\"play-btn\">PLAY NOW!</button></router-link>\n        </div>\n        <div class=\"find-user\">\n          <div class=\"find-user\">\n            <router-link to=\"/viewusers\"><button>View All Users</button></router-link>\n            <router-link to=\"/viewbarks\"><button>View Barks!</button></router-link>\n          </div>\n        </div>\n        <div class=\"user-watch\">\n          <h3 class=\"watch-header\">Your Watchboard:</h3>\n          <div v-if=\"has_watchlist\">\n            <div class=\"watch-sidescroll\">\n              <div class=\"watch-display\" :key=\"watch.id\" v-for=\"watch in watchlist\">\n                <div class=\"comp-div card\">\n                  <div class=\"comp-head\">\n                    <h4 class=\"competitor-name\"><b>Name: </b>{{ watch.handle }} </h4>\n                    <h5 class=\"competitor-score\"><b></b>Their Score: {{ watch.total_score }} </h5>\n                  </div>\n                  <button @click=\"removeFromList(watch.id, user_id)\">REMOVE </button>\n                  <button @click=\"makeBark(watch.id, watch.handle)\">BARK!</button>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div v-else class=\"user-watch\">\n            <h3 class=\"watch-header\">look like you're not watching anyone yet</h3>\n            <h4>Find some other players and watch their progress!</h4>\n            <div class=\"find-user\">\n              <router-link to=\"/viewusers\"><button>View All Users</button></router-link>\n            </div>\n          </div>\n        </div>\n        <div class=\"delete-yourself\">\n          <p>If you've had enough of RockDog trivia... go ahead and delete yourself. We'll be sad you\n            left. But we still appreciate you playing!</p>\n          <button class=\"delete-profile-btn\" @click=\"deleteUser()\">Delete Yourself</button>\n        </div>\n      </div>\n    </div>\n    <div v-else-if=\"!auth\" class=\"not-logged-in\">\n      <h1>You are not logged in!</h1>\n      <router-link to=\"/\"><button>\n          <h2>Go Back</h2>\n        </button></router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Client from '../services/api'\n\nexport default {\n  name: 'DashboardView',\n  props: {\n    user_handle: String,\n    user_id: Number,\n    auth: Boolean\n  },\n  data: () => ({\n    user_profile: {},\n    watchlist: [],\n    has_watchlist: false,\n    new_bark: false\n  }),\n  components: {},\n  mounted() {\n    this.getUserInfo()\n  },\n  methods: {\n    async getUserInfo() {\n      console.log(this.auth)\n      const intId = this.user_id\n      const Id = intId.toString()\n      const res = await Client.get(`/user/userprof/${Id}`)\n      this.user_profile = res.data\n      this.$emit('User', this.user_profile)\n      this.watchlist = res.data.being_watched\n      if (this.watchlist.length > 0) {\n        this.has_watchlist = true\n      } else {\n        this.has_watchlist = false\n      }\n    },\n    async deleteUser() {\n      const intId = this.user_id\n      await Client.delete(`/user/${intId}`)\n      this.$router.push(`/bye`)\n    },\n    async removeFromList(being_watched, watcher) {\n      await Client.delete(`/user/${being_watched}/${watcher}`)\n      this.getUserInfo()\n    },\n    makeBark(id, handle) {\n      this.$router.push(`/newbark/${id}/${handle}`)\n\n    }\n  }\n}\n</script>\n\n<style scoped>\n.profile-space {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.user-info {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n}\n\n.watch-header {\n  margin: 3vh 0 1vh 0;\n  text-shadow: 2px 1px 3px black;\n}\n\n.delete-yourself {\n  border-bottom: 1px solid black;\n  margin: 2vh 4vw;\n  padding-bottom: 2vh;\n}\n\n.watch-sidescroll {\n  display: flex;\n  flex-wrap: nowrap;\n  overflow-x: auto;\n  margin: 2vw\n}\n\n.card {\n  flex: 0 0 auto;\n}\n\n.comp-head {\n  height: 3vh;\n}\n\n.watch-display,\n.user-stats {\n  display: grid;\n  grid-template-columns: 1fr/1fr;\n  margin: 2vh 1vw 2vh 1vw;\n  box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.347);\n  border-radius: 6px;\n  background-color: rgba(240, 255, 255, 0.195);\n  padding: 2vw;\n}\n\n.user-stats {\n  margin: 2vh;\n  padding: 2vh\n}\n\n.play-btn-div {\n  margin: 2vh;\n}\n\n.play-btn {\n  width: 85vw;\n  height: 8vh;\n  background-color: rgb(244, 118, 28);\n  text-shadow: 2px 1px 3px;\n  font-size: 4vh;\n}\n\nbutton {\n  font-family: Commissioner;\n  width: 40vw;\n  height: 4vh;\n  background-color: rgba(227, 247, 247, 0.467);\n  border-radius: 6px;\n  margin: 1vh 1vw;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  margin: 0;\n  padding: 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import axios from 'axios';\n// const API_KEY = process.env.VUE_APP_AUDIODB_KEY\n\nexport default {\n  name: 'DashboardView',\n  props: {\n    user_handle: String,\n    user_id: Number\n  },\n  data: () => ({\n    user_profile: {},\n    watchlist: [],\n    has_watchlist: false\n  }),\n  components: {},\n  mounted() {\n    this.getUserInfo();\n  },\n  methods: {\n    async getUserInfo() {\n      const intId = this.user_id;\n      const Id = intId.toString();\n      const res = await axios.get(`http://localhost:3001/api/user/userprof/${Id}`);\n      console.log(res.data);\n      this.user_profile = res.data;\n      this.watchlist = res.data.barked;\n      console.log(this.watchlist);\n      if (this.watchlist.length > 0) {\n        this.has_watchlist = true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,KAAI,MAAO,OAAM;AACxB;;AAEA,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,WAAW,EAAEC,MAAM;IACnBC,OAAO,EAAEC;EACX,CAAC;EACDC,IAAI,EAAE,OAAO;IACXC,YAAY,EAAE,CAAC,CAAC;IAChBC,SAAS,EAAE,EAAE;IACbC,aAAa,EAAE;EACjB,CAAC,CAAC;EACFC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,GAAG;IACR,IAAI,CAACC,WAAW,EAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAW,GAAG;MAClB,MAAME,KAAI,GAAI,IAAI,CAACV,OAAM;MACzB,MAAMW,EAAC,GAAID,KAAK,CAACE,QAAQ,EAAC;MAC1B,MAAMC,GAAE,GAAI,MAAMlB,KAAK,CAACmB,GAAG,CAAE,2CAA0CH,EAAG,EAAC;MAC3EI,OAAO,CAACC,GAAG,CAACH,GAAG,CAACX,IAAI;MACpB,IAAI,CAACC,YAAW,GAAIU,GAAG,CAACX,IAAG;MAC3B,IAAI,CAACE,SAAQ,GAAIS,GAAG,CAACX,IAAI,CAACe,MAAK;MAC/BF,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,SAAS;MAC1B,IAAI,IAAI,CAACA,SAAS,CAACc,MAAK,GAAI,CAAC,EAAE;QAC7B,IAAI,CAACb,aAAY,GAAI,IAAG;MAC1B;IACF;EACF;AACF","names":["axios","name","props","user_handle","String","user_id","Number","data","user_profile","watchlist","has_watchlist","components","mounted","getUserInfo","methods","intId","Id","toString","res","get","console","log","barked","length"],"sourceRoot":"","sources":["/Users/jscarpelli/ga_seir919/projects/musical_pursuit/musicial_pursuit_front/musical_pursuit/src/components/Dashboard.vue"],"sourcesContent":["<template>\n  <div class=\"profile-space\">\n    <div class=\"user-info\">\n      <h1>{{ user_profile.handle }}</h1>\n      <h3>{{ user_profile.city }}</h3>\n    </div>\n    <div class=\"user-stats\">\n      <h1>{{ user_profile.current_level }}</h1>\n    </div>\n    <div class=\"user-watch\">\n      <h3 class=\"watch-header\">See who is barking at you!</h3>\n      <div v-if=\"has_barks\">\n        <div class=\"temp\">\n          <div class=\"watch-display\" :key=\"watch.id\" v-for=\"watch in watchlist\">\n            <span>\n              <h4>Your competitor:{{ watchlist.handle }}</h4>\n              <h5>Their level:{{ watchlist.total_score }}</h5>\n            </span>\n            <h6>What they had to say to you:</h6>\n            <p>{{ bark.Bark.bark }}</p>\n          </div>\n        </div>\n      </div>\n      <div v-else class=\"user-watch\">\n        <h3 class=\"watch-header\">look like you're not watcing anyone yet</h3>\n        <h4>Find some other players and watch their progress!</h4>\n        <button class=\"user-list-button\">Find Users</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n// const API_KEY = process.env.VUE_APP_AUDIODB_KEY\n\nexport default {\n  name: 'DashboardView',\n  props: {\n    user_handle: String,\n    user_id: Number\n  },\n  data: () => ({\n    user_profile: {},\n    watchlist: [],\n    has_watchlist: false\n  }),\n  components: {},\n  mounted() {\n    this.getUserInfo()\n  },\n  methods: {\n    async getUserInfo() {\n      const intId = this.user_id\n      const Id = intId.toString()\n      const res = await axios.get(`http://localhost:3001/api/user/userprof/${Id}`)\n      console.log(res.data)\n      this.user_profile = res.data\n      this.watchlist = res.data.barked\n      console.log(this.watchlist)\n      if (this.watchlist.length > 0) {\n        this.has_watchlist = true\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.profile-space {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.user-info {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n}\n\n.user-stats {\n  margin: 2vh;\n}\n\n.user-barks {\n  margin: 2vh;\n}\n\n.barks-display {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: flex-start;\n  /* align-content: flex-start; */\n}\n\n.bark-header {\n  margin-bottom: 2vh;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  margin: 0;\n  padding: 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}